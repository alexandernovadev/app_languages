{
  "expo": {
    "name": "app_llm_languages",
    "slug": "app_llm_languages",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#eeeeee"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIStatusBarHidden": true
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#eeeeee"
      },
      "statusBar": {
        "hidden": true,
        "translucent": true
      },
      "package": "com.alexsk88dev.app_llm_languages"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "f4e68b13-43eb-45a1-b1f2-5c980f638e99"
      }
    }
  }
}
